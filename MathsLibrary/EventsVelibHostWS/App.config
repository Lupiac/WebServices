<?xml version="1.0" encoding="utf-8" ?>
<configuration>
  <system.serviceModel>
    <bindings>
      <basicHttpBinding>
        <binding name="NewBinding0" maxBufferPoolSize="65536" maxBufferSize="200000000"
          maxReceivedMessageSize="2000000" />
      </basicHttpBinding>
    </bindings>
    <services>
      <service behaviorConfiguration="VelibWSBehavior" name="MathsLibrary.VelibWS">
        <endpoint address="" binding="wsDualHttpBinding" contract="MathsLibrary.IVelibWS" />
        <endpoint address="mex" binding="mexHttpBinding" contract="IMetadataExchange" />
        <host>
          <baseAddresses>
            <add baseAddress="http://localhost:9011/VelibWS" />
          </baseAddresses>
        </host>
      </service>
    </services>

    <!-- ********************************** behaviors ********************************** -->
    <behaviors>
      <serviceBehaviors>
        <!-- CalcService Behavior -->
        <behavior name="VelibWSBehavior">
          <serviceMetadata httpGetEnabled="true"/>
          <serviceDebug includeExceptionDetailInFaults="true "/>
        </behavior>
      </serviceBehaviors>
    </behaviors>

  </system.serviceModel>
</configuration>



